<?xml version='1.0' encoding='utf-8'?>
<widget id="com.attune.easyhelthcare" version="0.0.1" xmlns="http://www.w3.org/ns/widgets">
    <name>Easy Helthcare</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="loadUrlTimeoutValue" value="60000" />
    <preference name="FadeSplashScreenDuration" value="3000" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="GOOGLE_MAPS_ANDROID_API_KEY" value="AIzaSyBw0c8wX1cWa4jBq71dbG9wGqS3dsFINnY" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="loadUrlTimeoutValue" value="700000" />
        <icon density="ldpi" src="resources/android/icon/logo.png" />
        <icon density="mdpi" src="resources/android/icon/logo.png" />
        <icon density="hdpi" src="resources/android/icon/logo.png" />
        <icon density="xhdpi" src="resources/android/icon/logo.png" />
        <icon density="xxhdpi" src="resources/android/icon/logo.png" />
        <icon density="xxxhdpi" src="resources/android/icon/logo.png" />
        <splash density="land-ldpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="land-mdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="land-hdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="land-xhdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="land-xxhdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="land-xxxhdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="port-ldpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="port-mdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="port-hdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="port-xhdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="port-xxhdpi" src="resources/android/splash/01.Splash.jpg" />
        <splash density="port-xxxhdpi" src="resources/android/splash/01.Splash.jpg" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application" xmlns:android="http://schemas.android.com/apk/res/android">
            <application android:networkSecurityConfig="@xml/network_security_config" />
            <application android:usesCleartextTraffic="true" />
        </edit-config>
        <resource-file src="resources/android/xml/network_security_config.xml" target="app/src/main/res/xml/network_security_config.xml" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/icon.png" width="57" />
